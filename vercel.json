{
	"version": 2,
	"buildCommand": "npx pnpm install --no-frozen-lockfile && npx pnpm run build:all",
	"outputDirectory": "apps/blog/.next",
	"framework": "nextjs",
	"installCommand": "npm i -g pnpm && pnpm install --no-frozen-lockfile",
	"routes": [
		{
			"src": "/(.*)",
			"dest": "apps/blog/$1"
		}
	]
}